  "scripts": {
    "dev": "eval $(AWS_ENV_PATH=/internote/dev/ AWS_REGION=eu-west-1 ./aws-env --format=dotenv) next -p 3000",
    "dev:storybook": "start-storybook --quiet -p 6006 -s ./",
    "build:local:next": "rm -rf ./.next && eval $(AWS_ENV_PATH=/internote/dev/ AWS_REGION=eu-west-1 ./aws-env --format=dotenv) next build",
    "start:local:next": "eval $(AWS_ENV_PATH=/internote/dev/ AWS_REGION=eu-west-1 ./aws-env --format=dotenv) next start",
    "build:storybook": "rm -rf ./storybook-static && yarn build-storybook",
    "build:dev:create-service-worker": "STAGE=dev webpack --config service-worker-webpack.config.js",
    "deploy:dev:prepare": "rm -rf ./.next && yarn deploy:dev:environment-variables",
    "deploy:dev:environment-variables": "eval $(AWS_ENV_PATH=/internote/dev/ AWS_REGION=eu-west-1 ./aws-env --format=dotenv > .env)",
    "deploy:dev:build": "yarn build:dev:create-service-worker && yarn build:storybook",
    "deploy:dev": "yarn deploy:dev:prepare && yarn deploy:dev:build && serverless",
    "test": "jest",
    "test:tsc": "tsc --noEmit"
